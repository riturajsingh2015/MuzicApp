<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.riturajsingh.muzicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Now Playing" android:name=".NowPlayingActivity" />
        <activity android:label="Search" android:name=".SearchActivity" />
        <activity android:label="Store" android:name=".StoreActivity" />
        <activity android:label="Detail of Song " android:name=".DetailsOfSongActivity" />
        <activity android:label="Payment" android:name=".PaymentActivity"/>
    </application>

</manifest>